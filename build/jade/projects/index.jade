doctype html
!= "<html lang='en' xml:lang='en'>"
	head
		include ../templates/meta.jade

		meta(name='title', content='Ghis Bakour | Projects')
		meta(name='description', content="The best way to predict the future is to create it")

		// Facebook
		meta(property='og:title', content='Ghis Bakour | Projects')
		meta(property='og:description', content="The best way to predict the future is to create it")

		// Twitter
		meta(name='twitter:title', content='Ghis Bakour | Projects')
		meta(name='twitter:description', content="The best way to predict the future is to create it")

		include ../templates/head.jade

		title='Ghis Bakour | Projects'

		link(href='../images/logo-b.png', rel='icon', type='image/png')
		link(href='../css/style.css', rel='stylesheet', type='text/css')

	body
		.load
			!= '<svg width="60" height="100%" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 100" preserveAspectRatio="xMidYMid" class="lds-bars"><rect ng-attr-x="{{config.x1}}" y="30" ng-attr-width="{{config.width}}" height="40" fill="#ababab" x="15" width="10"><animate attributeName="opacity" calcMode="spline" values="1;0.2;1" keyTimes="0;0.5;1" dur="1" keySplines="0.5 0 0.5 1;0.5 0 0.5 1" begin="-0.6s" repeatCount="indefinite"></animate></rect><rect ng-attr-x="{{config.x2}}" y="30" ng-attr-width="{{config.width}}" height="40" fill="#000000" x="35" width="10"><animate attributeName="opacity" calcMode="spline" values="1;0.2;1" keyTimes="0;0.5;1" dur="1" keySplines="0.5 0 0.5 1;0.5 0 0.5 1" begin="-0.4s" repeatCount="indefinite"></animate></rect><rect ng-attr-x="{{config.x3}}" y="30" ng-attr-width="{{config.width}}" height="40" fill="#ababab" x="55" width="10"><animate attributeName="opacity" calcMode="spline" values="1;0.2;1" keyTimes="0;0.5;1" dur="1" keySplines="0.5 0 0.5 1;0.5 0 0.5 1" begin="-0.2s" repeatCount="indefinite"></animate></rect><rect ng-attr-x="{{config.x4}}" y="30" ng-attr-width="{{config.width}}" height="40" fill="#000000" x="75" width="10"><animate attributeName="opacity" calcMode="spline" values="1;0.2;1" keyTimes="0;0.5;1" dur="1" keySplines="0.5 0 0.5 1;0.5 0 0.5 1" begin="0s" repeatCount="indefinite"></animate></rect></svg>'

		main.main
			a.btn.btn--menu(href="#")
				img(src='../images/icons/icon-menu.svg')

			.overlay

			aside.menu.offside.offside--left
				a.logo(href='/')
					img(src='../images/logo-w.png'  alt='Ghis Bakour logo 2016'  width='100')

				nav.nav
					ul
						li: a.nav-hyper(href="/about")
							span About
							span The Creative Mind
						li: a.nav-hyper.nav-hyper--active(href="/projects")
							span Projects
							span My Exhibition Room
						li: a.nav-hyper(href="/talks")
							span Talks
							span Back to Community
						li: a.nav-hyper(href="/recognition")
							span Recognition
							span The Golden Trophies
						li: a.nav-hyper(href="/testimonials")
							span Testimonials
							span Coming Soon
						li: a.nav-hyper(href="/contact")
							span Contact
							span Getting in Touch

				.copyright

			section.projects.offside-content
				h1.label // Selection
				.arrow ↓

				article
					h1.headline PROJECTS
					hr
					h3.quattrocento &ldquo;The best way to predict the future is to create it&rdquo;

					h2 Case Studies
					.columns

						.column.project
							a.block(target='_blank' href='https://medium.com/@fatasuir9/docnotes-how-i-designed-the-process-of-hospital-prescription-writing-in-24-hours-ux-case-study-5de249a20127')
								img.project__screen(src='../images/projects/201603-project--engine.png' width='100%')

							.project__details
								h4 Engine - Design System
								p Building a convenient and centralised Design system that defines the brand’s known product and provide a starting point to explore new business opportunities in neighbouring territories.
								a(class="anchor" target="_blank" href='https://medium.com/@fatasuir9/engine-building-a-design-system-that-works-for-scale-in-the-auto-advertising-industry-ux-case-42a755ab2a3a') Read case study

						.column.project
							a.block(target='_blank' href='https://medium.com/@fatasuir9/docnotes-how-i-designed-the-process-of-hospital-prescription-writing-in-24-hours-ux-case-study-5de249a20127')
								img.project__screen(src='../images/projects/201606-project--docnotes.gif' width='100%')

							.project__details
								h4 Docnotes
								p Docnotes is a project I have worked on during the Angelhack KL '16 Hackathon.
								a(class="anchor" target="_blank" href='https://medium.com/@fatasuir9/docnotes-how-i-designed-the-process-of-hospital-prescription-writing-in-24-hours-ux-case-study-5de249a20127') Read case study

					.columns
						.column.project
							a.block(href='/projects/grypp')
								img.project__screen(src='../images/projects/201612-project--grypp.png' width='100%')

							.project__details
								h4 Grypp App
								p The ultimate goal is to focus on the simplicity of the interface design and let technology do the rest of the magic.
								a(class="anchor" target="_blank" href='/projects/grypp') Read case study

						.column


					h2 Highlights
					.columns
						.column.project
							img.project__screen(src='../images/projects/201811-project--isight.png' width='100%')
							.project__details
								h4 Project iSight
								p An all-in-one dashboard for OEMs in the automotive sector. It is customisable to view the local market or the SEA region.
								a(class="anchor" target="_blank" href='https://dribbble.com/shots/4664057-Project-iSight-Dashboard') View project


						.column.project
							img.project__screen(src='../images/projects/201812-project--icons.png' width='100%')
							.project__details
								h4 Video streaming icons
								p Icons set designed for be used for a live video streaming service!
								a(class="anchor" target="_blank" href='https://dribbble.com/shots/4645793-Live-video-streaming-icons') View project

					.columns
						.column.project
							img.project__screen(src='../images/projects/201805-project--ifttt.png' width='100%')
							.project__details
								h4 IFTTT Card Gallery
								p This is a design concept to showcase a minimalist approach to the applet gallery in IFTTT platform.


						.column.project
							img.project__screen(src='../images/projects/201710-project--caterspot.png' width='100%')
							.project__details
								h4 Caterspot Admin App
								p An app to allow vendors to manage upcoming caterspot orders, order notifications and or cancellation notice.
								a(class="anchor" target="_blank" href='https://www.caterspot.sg') View project

					.columns
						.column.project
							img.project__screen(src='../images/projects/201611-project--iflix.png' width='100%')
							.project__details
								h4 iFlix Dashboard
								p The project brief is to brainstorm, ideate and build a complete feature for the iFlix platform in 24hrs.
								a(class="anchor" target="_blank" href='https://sea.pcmag.com/iflix/12237/iflix-inaugural-hackathon-crowns-crowdsourcing-entry-as-top') Read the story

						.column.project
							img.project__screen(src='../images/projects/201701-project--calendar.png' width='100%')
							.project__details
								h4 Calendar Cards 2017
								p This is a concept calendar cards for the year of 2017. Each card has a black and white visuals of nature.

					.columns
						.column.project
							img.project__screen(src='../images/projects/201605-project--fetchlah.png' width='100%')
							.project__details
								h4 Fetchlah Project
								p The idea of this product is a one-stop for all pet lovers where they can find everything right from the nearest vets, services, cafes and or other activities.
								a(class="anchor" target="_blank" href='https://dribbble.com/shots/4416620-Fetchlah-Mobile-UI') View project


						.column.project
							img.project__screen(src='../images/projects/201604-project--carlist.png' width='100%')
							.project__details
								h4 The redesign of Carlist
								p The bottom part of the redesign project I've worked on for the Carlist platform.
								a.anchor(target="_blank" href='http://carlist.my') View project

					.columns
						.column.project
							img.project__screen(src='../images/projects/201602-project--morning.png' width='100%')
							.project__details
								h4 Morning Widget
								p Minimalist morning widget I've created a while back. The whole idea is to describe and show important information the moment you wake up in the morning.


						.column.project
							img.project__screen(src='../images/projects/201511-project--ringtonic.jpg' width='100%')
							.project__details
								h4 Ringtonic Branding
								p An ambitious project to give users the ability to download their favourite ringtones online directly to his/her device. Unfortunately, the project did not kick off due to limited resources.

					.columns
						.column.project
							img.project__screen(src='../images/projects/201507-project--stores.png' width='100%')
							.project__details
								h4 Online Stores Icons
								p Some of the major mobile platforms and the "download it from" badge.
								a(class="anchor" target="_blank" href='/projects/') Download it here

						.column.project
							img.project__screen(src='../images/projects/201503-project--rejuve.png' width='100%')
							.project__details
								h4 RejuveGroup - Desktop UI
								p Simple and minimalistic approach to viewing the limited edition products of dermal fillers.
								a(class="anchor" target="_blank" href='https://rejuvegroup.com') View project

					.columns
						.column.project
							img.project__screen(src='../images/projects/201502-project--rejuve.png' width='100%')
							.project__details
								h4 RejuveGroup logo
								p Branding strategy and logo design to attract customers' attention to building a trustworthy trademark in the beauty and health industry.

						.column.project
							img.project__screen(src='../images/projects/201410-project--android.png' width='100%')
							.project__details
								h4 Android Lock Screen
								p Simple and clean android lock screen concept, I was working in the past.


					.columns
						.column.project
							img.project__screen(src='../images/projects/201412-project--tc.png' width='100%')
							.project__details
								h4 Trustedcompany | B2B Dashboard
								p The admin dashboard for clients where they can view their customers' reviews and more. The feature set is limited for free trail users.
								a(class="anchor" target="_blank" href='https://dribbble.com/shots/2873721-Trustedcompany-com-B2B-2015') View project


						.column.project
							img.project__screen(src='../images/projects/201410-project--tc.png' width='100%')
							.project__details
								h4 Trustedcompany | B2C
								p A review platform built for online shoppers to review their online shopping experiences.
								a(class="anchor" target="_blank" href='https://dribbble.com/shots/2851415-Trustedcompany-com-Home-page-2015') View project


			include ../templates/social.jade

		include ../templates/footer.jade
!= "</html>"
